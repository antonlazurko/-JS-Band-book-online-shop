{  "parser": "babel-eslint",
    "extends": ["airbnb-base","plugin:import/errors",
    "plugin:import/warnings","plugin:sonarjs/recommended"],
    "plugins": ["react", "sonarjs"],
    "rules": {
        "sonarjs/cognitive-complexity": ["error", 30],
        "linebreak-style": 0,
        "dot-notation": 0,
        "no-console": 0,
        "max-len": ["error", { "code": 110 }]
  }
 }